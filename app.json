{
  "expo": {
    "name": "Event Flow",
    "slug": "event_flow",
    "version": "1.0.0",
    "description": "Event Flow - Manage your events, teams, and expenses efficiently. Organize and track all your activities in one place.",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "eventflow",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "author": "Zunore",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zunore.eventflow"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.zunore.eventflow",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": true,
          "android": {
            "enableFTS": false,
            "useSQLCipher": false
          },
          "ios": {
            "customBuildFlags": [
              "-DSQLITE_ENABLE_DBSTAT_VTAB=1 -DSQLITE_ENABLE_SNAPSHOT=1"
            ]
          }
        }
      ],
      "expo-secure-store",
      "expo-sqlite",
      "expo-notifications",
      "expo-background-task",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "05cb1182-32f4-43f8-8a9c-f75025e1e2ca"
      }
    },
    "owner": "baaszunnaiyyer",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/05cb1182-32f4-43f8-8a9c-f75025e1e2ca"
    },
    "notification": {
      "icon": "./assets/images/icon.png"
    }
  }
}
